


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title">List of pilot hired for this airline</h4>
</div>

<div class="modal-body">
    <div class="row" style="padding: 30px 35px 30px 35px;font-size:larger;">
        <ul>
            @foreach (var userStat in Model)
            {
                <li>@userStat.User.UserName</li>
            }
        </ul>
    </div>
</div>
